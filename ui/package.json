{
  "name": "@fe/ui",
  "version": "0.1.0",
  "description": "",
  "main": "./out/main/index.js",
  "scripts": {
    "format": "prettier --write .",
    "eslint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "lint": "pnpm run typecheck && pnpm run eslint",
    "lint:watch": "cargo watch -C . --skip-local-deps --why -- pnpm run lint",
    "start": "electron-vite preview -- --enable-features=UseOzonePlatform",
    "dev": "electron-vite dev -- --enable-features=UseOzonePlatform",
    "dev:watch": "electron-vite dev --watch -- --enable-features=UseOzonePlatform",
    "build": "pnpm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "pnpm run build && electron-builder --dir",
    "build:win": "pnpm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "test": "vitest --run",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@atlaskit/pragmatic-drag-and-drop": "^1.7.4",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@fe/native": "workspace:*",
    "@kobalte/core": "^0.13.11",
    "@solid-primitives/deep": "^0.3.3",
    "@solid-primitives/destructure": "^0.2.2",
    "@solid-primitives/storage": "^4.3.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "color": "^5.0.0",
    "effect": "~3.17.1",
    "electron-updater": "^6.3.9",
    "solid-effect": "^1.0.0",
    "solid-motionone": "^1.0.4",
    "solid-notifications": "^1.2.0",
    "solid-styled-components": "^0.28.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tsafe": "^1.8.5",
    "uuid": "^11.1.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@effect/vitest": "~0.25.1",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@eslint/js": "^9.32.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.1.12",
    "@types/node": "^22.16.5",
    "electron": "^37.2.3",
    "electron-builder": "^25.1.8",
    "electron-vite": "^4.0.0",
    "eslint": "^9.32.0",
    "eslint-plugin-solid": "^0.14.5",
    "glob": "^11.0.3",
    "jsdom": "^26.1.0",
    "solid-js": "^1.9.9",
    "tailwindcss": "^4.1.11",
    "ts-morph": "^26.0.0",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.5",
    "vite-plugin-solid": "^2.11.7",
    "vite-plugin-solid-svg": "^0.8.1",
    "vitest": "^3.2.4"
  }
}
